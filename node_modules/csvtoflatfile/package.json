{
  "name": "csvtoflatfile",
  "version": "2.0.0",
  "description": "Converts a csv to a Flatfile",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "scripts": {
    "compile": "tsc",
    "dev": "node --no-warnings=ExperimentalWarning --loader ts-node/esm src/demo/example.ts",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BrycePearce/CsvToFlatfile.git"
  },
  "keywords": [
    "flatfile",
    "csv"
  ],
  "files": [
    "dist/*",
    "@types/*"
  ],
  "author": "Bryce Pearce",
  "license": "MIT",
  "dependencies": {
    "csv": "^6.3.1",
    "got": "^13.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "3.0.0"
  }
}
